{
    "root": true,
    "extends": [
        "wikimedia/common",
        "wikimedia/language/es2020"
    ],
    "rules": {
        "comma-dangle": "off",
        "no-implicit-globals": "off",
        "indent": [ "error", 4 ],
        "operator-linebreak": [ "error", "before" ]
    },
    "env": {
        "browser": true
    }
}
